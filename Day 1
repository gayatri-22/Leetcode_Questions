118. Pascal's Triangle

class Solution {
    public List<List<Integer>> generate(int numRows) {
       List<List<Integer>> res= new ArrayList<List<Integer>>();
       for(int i=0;i<numRows;i++){
           List<Integer> val= new ArrayList<>();
           double x=1.0;
           for(int j=0;j<=i;j++){
               val.add((int)x);
               x = x * (i-j)/(j+1);         
           }
           res.add(val); 
       }
       return res;
    }
}


